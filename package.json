    "test:chrome": "testcafe chrome  ./Zomato_Online/tests  --reporter allure,html:HTML-Reports/zomato.html -s takeOnFails=true,path=artifacts/screenshots/zomato,pathPattern='${DATE}_${TIME}/test-${TEST_INDEX}/${USERAGENT}/${TEST}_${FILE_INDEX}.png' --skip-js-errors --disable-page-caching ,spec --video artifacts/videos --video-options singleFile=true",
